{
  "name": "social-from-email",
  "version": "1.0.0",
  "description": "Given an email, return best-effort social profile URLs if publicly discoverable (Gravatar + heuristics).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "enhanced-person-analysis": "./dist/cli-enhanced-person-analysis.cjs",
    "crawlee-person-analysis": "./dist/cli-crawlee-person-analysis.cjs",
    "hybrid-person-analysis": "./dist/cli-hybrid-person-analysis.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli-enhanced-person-analysis.ts src/cli-crawlee-person-analysis.ts src/cli-hybrid-person-analysis.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts src/cli-enhanced-person-analysis.ts src/cli-crawlee-person-analysis.ts src/cli-hybrid-person-analysis.ts --dts --format esm,cjs --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "email",
    "social",
    "osint",
    "gravatar"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@crawlee/cheerio": "^3.14.1",
    "@crawlee/jsdom": "^3.14.1",
    "@crawlee/playwright": "^3.14.1",
    "@crawlee/puppeteer": "^3.14.1",
    "@types/puppeteer": "^7.0.4",
    "all-the-cities": "^3.1.0",
    "commander": "^9.5.0",
    "compromise": "^14.14.4",
    "crawlee": "^3.14.1",
    "css-selector-generator": "^3.6.9",
    "dotenv": "^17.2.1",
    "google-sr-selectors": "^3.0.0",
    "i18n-iso-countries": "^7.14.0",
    "keyword-extractor": "^0.0.28",
    "ml-kmeans": "^6.0.0",
    "moment-timezone": "^0.6.0",
    "natural": "^8.1.0",
    "playwright": "^1.54.2",
    "puppeteer": "^24.16.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tree-kill": "^1.2.2"
  }
}
